{
  "name": "team-scoring-2024",
  "private": true,
  "version": "2.0.1-web-worker-exp",
  "type": "module",
  "engines": {
    "npm": ">=10.8.2",
    "node": ">=20.17.0",
    "yarn": ">=1.22.22"
  },
  "scripts": {
    "dev": "vite --port 3001 --open",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite --port 3000 --open preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "build:prod": "tsc && VITE_BUILD_DATE=$(date +\"%Y-%m-%d %T\") VITE_APP_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g') VITE_GIT_SHA1=$(git show --format=\"%h\" --no-patch) VITE_GIT_BRANCH_NAME=$(git rev-parse --abbrev-ref HEAD) vite build --mode production --emptyOutDir",
    "_deploy2:staging": "yarn build:prod && bash _aux-tool.deploy2.prod.sh",
    "_pwa:assets-gen": "pwa-assets-generator --preset minimal-2023 public/pwa/sloth-circle.svg"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.0",
    "@mui/material": "^6.3.0",
    "@mui/x-date-pickers": "^7.23.3",
    "@statelyai/inspect": "^0.4.0",
    "@xstate/react": "^5.0.1",
    "@xstate/store": "^2.6.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "dayjs-business-time": "^1.0.4",
    "invert-color": "^2.0.0",
    "javascript-time-ago": "^2.5.11",
    "math-interpolate": "^5.0.0",
    "react": "^18.3.1",
    "react-countdown": "^2.3.6",
    "react-dom": "^18.3.1",
    "react-google-charts": "^5.2.1",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "6.x",
    "react-time-ago": "^7.3.3",
    "string-to-color": "^2.2.2",
    "todomvc-app-css": "^2.4.3",
    "todomvc-common": "^1.0.5",
    "xstate": "^5.19.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "2.1.8",
    "@vitest/web-worker": "^2.1.8",
    "browserslist-to-esbuild": "^2.1.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass-embedded": "^1.83.0",
    "slugify": "^1.6.6",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^2.1.8"
  }
}
