{
  "source": {
    "include": [
      "./public/static/workers"
    ],
    "includePattern": ".+\\.(j|t)s(doc|x)?$"
  },
  "opts": {
    "destination": "./public/jsdoc/clean-jsdoc-theme",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "include_css": [
        "./tools.jsdoc/clean-jsdoc-theme/theme-fix.css"
      ],
      "include_js": [
        "./tools.jsdoc/clean-jsdoc-theme/core-fix.js"
      ]
    }
  },
  "templates": {
    "default": {
      "layoutFile": "./tools.jsdoc/clean-jsdoc-theme/layout.tmpl",
      "staticFiles": {}
    }
  }
}