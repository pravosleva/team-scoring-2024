{
  "source": {
    "include": [
      "./src/shared",
      "./public/static/workers"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./public/jsdoc/docdash",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/docdash"
  },
  "templates": {
    "default": {
      "layoutFile": "./jsdoc-tools/docdash/layout.tmpl",
      "staticFiles": {
        "include": [
          "node_modules/docdash/static/styles/jsdoc.css",
          "node_modules/docdash/static/styles/prettify.css",
          "./jsdoc-tools/docdash/theme-fix.css"
        ]
      }
    }
  }
}